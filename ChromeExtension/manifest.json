{
  "manifest_version": 2,

  "name": "VR-One Plugin",
  "description": "This extension provides utitilies to be used for VR-One",
  "version": "1.0",
    "content_security_policy":"script-src 'self' vr-one-4e3bb.firebaseapp.com; object-src 'self'",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },

  "browser_action": {
   "default_icon": "camera.png",
   "default_title": "take a screenshot"
  },
  "permissions": [
   "activeTab"
   ]
}